{
  "name": "event-culture-submit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm rebuild lightningcss || true",
    "dev": "next dev",
    "dev:UAT": "cross-env NODE_ENV=test next dev",
    "dev:PRODUCTION": "cross-env NODE_ENV=production next dev",
    "build:DEVELOPMENT": "cross-env NODE_ENV=development next build",
    "build:UAT": "cross-env NODE_ENV=test next build",
    "build": "next build",
    "postbuild:DEVELOPMENT": "npm run copy-standalone-assets",
    "postbuild:UAT": "npm run copy-standalone-assets",
    "postbuild": "npm run copy-standalone-assets",
    "copy-standalone-assets": "shx mkdir -p build/standalone/build/static && shx cp -r build/static/* build/standalone/build/static && shx mkdir -p build/standalone/public && shx cp -r public/* build/standalone/public",
    "start:without-standalone": "next start",
    "start": "node build/standalone/server.js",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typegen": "next typegen"
  },
  "dependencies": {
    "@bprogress/next": "^3.2.12",
    "@heroui/react": "^2.8.7",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-direction": "^1.1.1",
    "@reduxjs/toolkit": "^2.11.2",
    "country-flag-icons": "^1.6.12",
    "cross-env": "^10.1.0",
    "framer-motion": "^12.23.26",
    "google-translate-api-x": "^10.7.2",
    "libphonenumber-js": "^1.12.36",
    "lucide-react": "^0.563.0",
    "next": "^16.1.1",
    "next-intl": "^4.6.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "rehype-raw": "^7.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "swiper": "^12.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-google-recaptcha": "^2.1.9",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "shx": "^0.4.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
